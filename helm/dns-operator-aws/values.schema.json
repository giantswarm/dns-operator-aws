{
    "$schema": "http://json-schema.org/schema#",
    "type": "object",
    "properties": {
        "associateResolverRules": {
            "type": "boolean"
        },
        "aws": {
            "type": "object",
            "properties": {
                "accessKeyID": {
                    "type": "string"
                },
                "region": {
                    "type": "string"
                },
                "secretAccessKey": {
                    "type": "string"
                }
            }
        },
        "image": {
            "type": "object",
            "properties": {
                "name": {
                    "type": "string"
                },
                "tag": {
                    "type": "string"
                }
            }
        },
        "managementClusterName": {
            "type": "string"
        },
        "managementClusterNamespace": {
            "type": "string"
        },
        "pod": {
            "type": "object",
            "properties": {
                "group": {
                    "type": "object",
                    "properties": {
                        "id": {
                            "type": "integer"
                        }
                    }
                },
                "user": {
                    "type": "object",
                    "properties": {
                        "id": {
                            "type": "integer"
                        }
                    }
                }
            }
        },
        "project": {
            "type": "object",
            "properties": {
                "branch": {
                    "type": "string"
                },
                "commit": {
                    "type": "string"
                }
            }
        },
        "registry": {
            "type": "object",
            "properties": {
                "domain": {
                    "type": "string"
                }
            }
        },
        "resolverRulesOwnerAccount": {
            "type": "string"
        },
        "verticalPodAutoscaler": {
            "type": "object",
            "properties": {
                "enabled": {
                    "type": "boolean"
                }
            }
        }
    }
}
